{
  "name": "restflow",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{js,ts,vue,css,scss,html,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,vue,css,scss,html,json}\"",
    "generate:types": "cd ../backend && cargo test export_bindings",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.8.0",
    "@vue-flow/background": "^1.3.2",
    "@vue-flow/controls": "^1.1.3",
    "@vue-flow/core": "^1.45.1",
    "@vue-flow/minimap": "^1.5.3",
    "@vueuse/core": "^13.8.0",
    "axios": "^1.11.0",
    "dompurify": "^3.2.7",
    "element-plus": "^2.10.6",
    "lucide-vue-next": "^0.542.0",
    "marked": "^16.3.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@tauri-apps/cli": "^2.0.0-rc.18",
    "@types/dompurify": "^3.0.5",
    "@types/marked": "^5.0.2",
    "@types/node": "^24.3.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "axios-mock-adapter": "^2.1.0",
    "happy-dom": "^19.0.2",
    "prettier": "^3.6.2",
    "sass": "^1.91.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.5"
  }
}
