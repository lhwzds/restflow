{
  "templates": [
    {
      "id": "langgraph_simple",
      "name": "LangGraph - Simple Graph",
      "description": "Basic LangGraph example with a single processing node",
      "category": "LangGraph",
      "difficulty": "beginner",
      "file": "langgraph_simple.py",
      "dependencies": [
        "langgraph>=1.0",
        "langchain>=1.0"
      ],
      "inputSchema": {
        "type": "object",
        "properties": {
          "messages": {
            "type": "array",
            "items": { "type": "string" }
          }
        }
      },
      "outputSchema": {
        "type": "object",
        "properties": {
          "messages": { "type": "array" },
          "result": { "type": "string" }
        }
      }
    },
    {
      "id": "langgraph_react_agent",
      "name": "LangGraph - ReAct Agent",
      "description": "ReAct agent with OpenAI integration and message handling",
      "category": "LangGraph",
      "difficulty": "intermediate",
      "file": "langgraph_react_agent.py",
      "dependencies": [
        "langgraph>=1.0",
        "langchain>=1.0",
        "langchain-openai>=0.1.0"
      ],
      "inputSchema": {
        "type": "object",
        "properties": {
          "messages": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "role": { "type": "string" },
                "content": { "type": "string" }
              }
            }
          }
        }
      },
      "outputSchema": {
        "type": "object",
        "properties": {
          "messages": { "type": "array" },
          "next_action": { "type": "string" }
        }
      },
      "envVars": ["OPENAI_API_KEY"]
    },
    {
      "id": "langgraph_multi_agent",
      "name": "LangGraph - Multi-Agent System",
      "description": "Collaborative multi-agent system with researcher and writer agents",
      "category": "LangGraph",
      "difficulty": "advanced",
      "file": "langgraph_multi_agent.py",
      "dependencies": [
        "langgraph>=1.0",
        "langchain>=1.0",
        "langchain-openai>=0.1.0"
      ],
      "inputSchema": {
        "type": "object",
        "properties": {
          "task": { "type": "string" }
        },
        "required": ["task"]
      },
      "outputSchema": {
        "type": "object",
        "properties": {
          "task": { "type": "string" },
          "research_output": { "type": "string" },
          "writer_output": { "type": "string" },
          "final_output": { "type": "string" }
        }
      },
      "envVars": ["OPENAI_API_KEY"]
    }
  ]
}
