{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.restflow.ai/api/:path*"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
