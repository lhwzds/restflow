  "dependencies": {
    "@tauri-apps/api": "^2.8.0",
    "@vue-flow/background": "^1.3.2",
    "@vue-flow/controls": "^1.1.3",
    "@vue-flow/core": "^1.45.1",
    "@vue-flow/minimap": "^1.5.3",
    "@vueuse/core": "^13.8.0",
    "axios": "^1.11.0",
    "dompurify": "^3.2.7",
    "element-plus": "^2.11.4",
    "lucide-vue-next": "^0.542.0",
    "marked": "^16.3.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@tauri-apps/cli": "^2.0.0-rc.18",
    "@types/dompurify": "^3.2.0",
    "@types/marked": "^5.0.2",
    "@types/node": "^24.7.1",
