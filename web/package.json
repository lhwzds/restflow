    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "axios-mock-adapter": "^2.1.0",
    "happy-dom": "^20.0.2",
    "msw": "^2.11.5",
    "prettier": "^3.6.2",
    "sass": "^1.93.2",
    "typescript": "~5.9.3",
    "vite": "^7.1.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.1"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
