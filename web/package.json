{
  "name": "@restflow/web",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{js,ts,vue,css,scss,html,json}\"",
