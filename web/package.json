{
  "name": "@restflow/web",
  "description": "RestFlow - AI Agent Workflow Automation",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "postbuild": "node -e \"const fs=require('fs');const path=require('path');const target=path.join(process.cwd(),'dist','.gitkeep');fs.mkdirSync(path.dirname(target),{recursive:true});fs.closeSync(fs.openSync(target,'a'));\"",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{js,ts,vue,css,scss,html,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,vue,css,scss,html,json}\"",
    "generate:types": "cd ../crates/restflow-core && cargo test export_bindings",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.19.1",
    "@codemirror/commands": "^6.10.2",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/language": "^6.11.3",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.39.14",
    "@tailwindcss/vite": "^4.1.18",
    "@tauri-apps/api": "^2.9.1",
    "@vue-flow/background": "^1.3.2",
    "@vue-flow/controls": "^1.1.3",
    "@vue-flow/core": "^1.47.0",
    "@vue-flow/minimap": "^1.5.3",
    "@vueuse/core": "^14.2.0",
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/addon-unicode11": "^0.9.0",
    "@xterm/addon-webgl": "^0.19.0",
    "@xterm/xterm": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dompurify": "^3.2.7",
    "lucide-vue-next": "^0.563.0",
    "marked": "^17.0.0",
    "pinia": "^3.0.4",
    "radix-vue": "^1.9.17",
    "shiki": "^3.22.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "vue": "^3.5.28",
    "vue-i18n": "^12.0.0-alpha.3",
    "vue-router": "^5.0.2",
    "vue-sonner": "^2.0.9"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/dompurify": "^3.2.0",
    "@types/marked": "^6.0.0",
    "@types/node": "^25.2.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^4.0.13",
    "@vitest/ui": "^4.0.13",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "happy-dom": "^20.0.2",
    "msw": "^2.11.6",
    "prettier": "^3.6.2",
    "sass": "^1.93.2",
    "typescript": "~5.9.3",
    "vite": "^7.2.6",
    "vitest": "^4.0.13",
    "vue-tsc": "^3.1.5"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
